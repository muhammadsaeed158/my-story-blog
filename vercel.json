{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/videos", "dest": "/videos.html" },
    { "src": "/articles", "dest": "/articles.html" },
    { "src": "/stories", "dest": "/stories.html" },
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}